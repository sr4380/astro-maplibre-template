{
  "version": 8,
  "name": "custom-toner",
  "metadata": {"maputnik:renderer": "mlgljs"},
  "sources": {
    "protomaps": {
      "attribution": "<a href=\"https://github.com/protomaps/basemaps\">Protomaps</a> Â© <a href=\"https://openstreetmap.org\">OpenStreetMap</a>",
      "type": "vector",
      "tiles": [
        "https://api.protomaps.com/tiles/v3/{z}/{x}/{y}.mvt?key=46fe7633b5463e2b"
      ],
      "maxzoom": 15
      }
  },
  "sprite": "",
  "glyphs": "https://protomaps.github.io/basemaps-assets/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "minzoom": 2,
      "maxzoom": 22,
      "paint": {
        "background-color": "#00aaff"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "water",
      "paint": {
        "fill-color": "#000000"
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "landuse",
      "paint": {
        "fill-color": "#ffffff"
      }
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "protomaps",
      "source-layer": "building",
      "paint": {
        "fill-color": "#000000"
      }
    },
    {
      "id": "roads",
      "type": "line",
      "source": "protomaps",
      "source-layer": "transportation",
      "paint": {
        "line-color": "#000000",
        "line-width": 0.8
      }
    }
  ]
}
